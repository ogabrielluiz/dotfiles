# API Keys and Sensitive Environment Variables Template
# Copy this file to ~/.api_keys and add your actual keys
# chmod 600 ~/.api_keys

# OpenAI
# export OPENAI_API_KEY=your_openai_key_here

# Anthropic
# export ANTHROPIC_API_KEY=your_anthropic_key_here

# Tavily
# export TAVILY_API_KEY=your_tavily_key_here

# Groq
# export GROQ_API_KEY=your_groq_key_here

# NVIDIA
# export NVIDIA_API_KEY=your_nvidia_key_here

# Composio
# export COMPOSIO_API_KEY=your_composio_key_here

# Hugging Face
# export HUGGINFACE_API_TOKEN=your_hf_token_here

# Astra DB
# export ASTRA_DB_APPLICATION_TOKEN=your_astra_token_here
# export ASTRA_DB_API_ENDPOINT=your_astra_endpoint_here

# CodeFlash
# export CODEFLASH_API_KEY=your_codeflash_key_here

# AWS Profile
# export AWS_PROFILE=your_aws_profile_here